{
    "compilerOptions": {
        "target": "es5",
        "moduleResolution": "node",
        "lib": [
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "outDir": "dist/",
        "baseUrl": ".",
        "allowJs": true,
        "declaration": true,
        "sourceMap": false,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
    },
    "include": [
        "src/**/*"
    ],
}